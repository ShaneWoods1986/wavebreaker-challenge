{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": "react-app",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    }
}